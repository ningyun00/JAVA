package forXunHuan;

public class For01 {

	public static void main(String[] args) {
		for (int i = 1; i <= 10; i++) {//лл
//			System.out.print("@");
			for (int j = 1; j <= 10; j++) {//┴л
				System.out.print("");
				if (i>=1&&i<=10) {
					System.out.print("1");
				} else if(j>=2||j<=9) {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
	
}
