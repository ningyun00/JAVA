package forXunHuan;

public class For01 {

	public static void main(String[] args) {
		for (int i = 1; i <= 3; i++) {//лл
			for (int j = 1; j <= 3; j++) {//┴л
//				System.out.print("*");
				if (i==1&&i==2) {
					System.out.print(" ");
				}else if(j==2||j==3) {
					System.out.print("&");
				}
			}
			System.out.println();
		}
	}
	
}
